@{
}

<table>
    <thead>
        <tr>
            <th>Titulo</th>
            <th>Personaje</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var mision in ViewBag.Lista)
        {
            <tr>
                <td>@mision.Titulo</td>
                <td>@mision.Personaje.Nombre</td>

            </tr>
        }
    </tbody>
</table>